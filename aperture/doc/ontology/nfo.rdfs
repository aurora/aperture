<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE rdf:RDF [
	 <!ENTITY rdf 'http://www.w3.org/1999/02/22-rdf-syntax-ns#'>
	 <!ENTITY dc 'http://purl.org/dc/elements/1.1/'>
	 <!ENTITY a 'http://protege.stanford.edu/system#'>
	 <!ENTITY nfo 'http://www.semanticdesktop.org/ontologies/2007/03/22/nfo#'>
	 <!ENTITY nco 'http://www.semanticdesktop.org/ontologies/2007/03/22/nco#'>
	 <!ENTITY xsd 'http://www.w3.org/2001/XMLSchema#'>
	 <!ENTITY nie 'http://www.semanticdesktop.org/ontologies/2007/01/19/nie#'>
	 <!ENTITY rdfs 'http://www.w3.org/2000/01/rdf-schema#'>
]>
<rdf:RDF xmlns:rdf="&rdf;"
	 xmlns:nfo="&nfo;"
	 xmlns:a="&a;"
	 xmlns:dc="&dc;"
	 xmlns:nco="&nco;"
	 xmlns:xsd="&xsd;"
	 xmlns:rdfs="&rdfs;"
	 xmlns:nie="&nie;">
<rdfs:Class rdf:about="&nfo;Application"
	 rdfs:comment="An application"
	 rdfs:label="Application">
	<rdfs:subClassOf rdf:resource="&nfo;Software"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;Archive"
	 rdfs:label="Archive">
	<rdfs:comment>A compressed file. May contain other files or folder inside. </rdfs:comment>
	<rdfs:subClassOf rdf:resource="&nfo;DataContainer"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;ArchiveItem"
	 rdfs:comment="A file entity inside an archive."
	 rdfs:label="ArchiveItem">
	<rdfs:subClassOf rdf:resource="&nfo;EmbeddedFileDataObject"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;Attachment"
	 rdfs:label="Attachment">
	<rdfs:comment>A file attached to another data object. Many data formats allow for attachments: emails, vcards, ical events, id3 and exif...</rdfs:comment>
	<rdfs:subClassOf rdf:resource="&nfo;EmbeddedFileDataObject"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;Audio"
	 rdfs:comment="A file containing audio content"
	 rdfs:label="Audio">
	<rdfs:subClassOf rdf:resource="&nfo;Media"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;CompressionType"
	 rdfs:label="CompressionType">
	<rdfs:comment>Type of compression. Instances of this class represent the limited set of values allowed for the nfo:compressionType property.</rdfs:comment>
	<rdfs:subClassOf rdf:resource="&rdfs;Resource"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;Cursor"
	 rdfs:comment="A Cursor."
	 rdfs:label="Cursor">
	<rdfs:subClassOf rdf:resource="&nfo;RasterImage"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;DataContainer"
	 rdfs:label="DataContainer">
	<rdfs:comment xml:space='preserve'><![CDATA[A superclass for all entities, whose primary purpose is to serve as containers for other data object. They usually don't have any "meaning" by themselves. Examples include folders, archives and optical disc images.]]></rdfs:comment>
	<rdfs:subClassOf rdf:resource="&nie;InformationElement"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;DeletedResource"
	 rdfs:label="DeletedResource">
	<rdfs:comment>A file entity that has been deleted from the original source. Usually such entities are stored within various kinds of 'Trash' or 'Recycle Bin' folders.</rdfs:comment>
	<rdfs:subClassOf rdf:resource="&nfo;FileDataObject"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;Document"
	 rdfs:label="Document">
	<rdfs:comment>A generic document. A common superclass for all documents on the desktop.</rdfs:comment>
	<rdfs:subClassOf rdf:resource="&nie;InformationElement"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;EmbeddedFileDataObject"
	 rdfs:label="EmbeddedFileDataObject">
	<rdfs:comment>A file embedded in another data object. There are many ways in which a file may be embedded in another one. Use this class directly only in cases if none of the subclasses gives a better description of your case.</rdfs:comment>
	<rdfs:subClassOf rdf:resource="&nfo;FileDataObject"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;Executable"
	 rdfs:comment="An executable file."
	 rdfs:label="Executable">
	<rdfs:subClassOf rdf:resource="&nie;InformationElement"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;FileDataObject"
	 rdfs:label="FileDataObject">
	<rdfs:comment>A resource containing a finite sequence of bytes with arbitrary information, that is available to a computer program and is usually based on some kind of durable storage. A file is durable in the sense that it remains available for programs to use after the current program has finished.</rdfs:comment>
	<rdfs:subClassOf rdf:resource="&nie;DataObject"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;FileHash"
	 rdfs:label="FileHash">
	<rdfs:comment>A fingerprint of the file, generated by some hashing function.</rdfs:comment>
	<rdfs:subClassOf rdf:resource="&rdfs;Resource"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;Filesystem"
	 rdfs:label="Filesystem">
	<rdfs:comment>A filesystem. Examples of filesystems include hard disk partitions, removable media, but also images thereof stored in files.</rdfs:comment>
	<rdfs:subClassOf rdf:resource="&nfo;DataContainer"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;FilesystemImage"
	 rdfs:label="FilesystemImage">
	<rdfs:comment>An image of a filesystem. Instances of this class may include CD images, DVD images or hard disk partition images created by various pieces of software (e.g. Norton Ghost)</rdfs:comment>
	<rdfs:subClassOf rdf:resource="&nfo;Filesystem"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;Folder"
	 rdfs:label="Folder">
	<rdfs:comment>A folder/directory. Examples of folders include folders on a filesystem and message folders in a mailbox.</rdfs:comment>
	<rdfs:subClassOf rdf:resource="&nfo;DataContainer"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;Font"
	 rdfs:comment="A font."
	 rdfs:label="Font">
	<rdfs:subClassOf rdf:resource="&nie;InformationElement"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;HardDiskPartition"
	 rdfs:comment="A partition on a hard disk"
	 rdfs:label="HardDiskPartition">
	<rdfs:subClassOf rdf:resource="&nie;DataObject"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;HtmlDocument"
	 rdfs:comment="A HTML document, may contain links to other files."
	 rdfs:label="HtmlDocument">
	<rdfs:subClassOf rdf:resource="&nfo;PlainTextDocument"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;Icon"
	 rdfs:label="Icon">
	<rdfs:comment>An Icon (regardless of whether it's a raster or a vector icon. A resource representing an icon could have two types (Icon and Raster, or Icon and Vector) if required.</rdfs:comment>
	<rdfs:subClassOf rdf:resource="&nfo;Image"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;Image"
	 rdfs:comment="A file containing an image."
	 rdfs:label="Image">
	<rdfs:subClassOf rdf:resource="&nfo;Visual"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;Media"
	 rdfs:label="Media">
	<rdfs:comment>A piece of media content. This class may be used to express complex media containers with many streams of various media content (both aural and visual).</rdfs:comment>
	<rdfs:subClassOf rdf:resource="&nie;InformationElement"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;MediaFileListEntry"
	 rdfs:label="MediaFileListEntry">
	<rdfs:comment>A single node in the list of media files contained within an MediaList instance. This class is intended to provide a type all those links have. In valid NRL untyped resources cannot be linked. There are no properties defined for this class but the application may expect rdf:first and rdf:last links. The former points to the DataObject instance, interpreted as Media the latter points at another MediaFileListEntr. At the end of the list there is a link to rdf:nil.</rdfs:comment>
	<rdfs:subClassOf rdf:resource="&rdfs;Resource"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;MediaList"
	 rdfs:comment="A file containing a list of media files.e.g. a playlist"
	 rdfs:label="MediaList">
	<rdfs:subClassOf rdf:resource="&nie;InformationElement"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;MediaStream"
	 rdfs:label="MediaStream">
	<rdfs:comment>A stream of multimedia content, usually contained within a media container such as a movie (containing both audio and video) or a DVD (possibly containing many streams of audio and video). Most common interpretations for such a DataObject include Audio and Video.</rdfs:comment>
	<rdfs:subClassOf rdf:resource="&nie;DataObject"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;MindMap"
	 rdfs:label="MindMap">
	<rdfs:comment>A MindMap, created by a mind-mapping utility. Examples might include FreeMind or mind mapper.</rdfs:comment>
	<rdfs:subClassOf rdf:resource="&nfo;Document"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;OperatingSystem"
	 rdfs:comment="An OperatingSystem"
	 rdfs:label="OperatingSystem">
	<rdfs:subClassOf rdf:resource="&nfo;Software"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;PaginatedTextDocument"
	 rdfs:label="PaginatedTextDocument">
	<rdfs:comment>A file containing a text document, that is unambiguously divided into pages. Examples might include PDF, DOC, PS, DVI etc.</rdfs:comment>
	<rdfs:subClassOf rdf:resource="&nfo;TextDocument"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;PlainTextDocument"
	 rdfs:label="PlainTextDocument">
	<rdfs:comment>A file containing plain text (ASCII, Unicode or other encodings). Examples may include TXT, HTML, XML, program source code etc.</rdfs:comment>
	<rdfs:subClassOf rdf:resource="&nfo;TextDocument"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;Presentation"
	 rdfs:label="Presentation">
	<rdfs:comment>A Presentation made by some presentation software (Corel Presentations, OpenOffice Impress, MS Powerpoint etc.)</rdfs:comment>
	<rdfs:subClassOf rdf:resource="&nfo;Document"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;RasterImage"
	 rdfs:comment="A raster image."
	 rdfs:label="RasterImage">
	<rdfs:subClassOf rdf:resource="&nfo;Image"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;RemoteDataObject"
	 rdfs:label="RemoteDataObject">
	<rdfs:comment>A file data object stored at a remote location. Don't confuse this class with a RemotePortAddress. This one applies to a particular resource, RemotePortAddress applies to an address, that can have various interpretations.</rdfs:comment>
	<rdfs:subClassOf rdf:resource="&nfo;FileDataObject"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;RemotePortAddress"
	 rdfs:label="RemotePortAddress">
	<rdfs:comment>An address specifying a remote host and port. Such an address can be interpreted in many ways (examples of such interpretations include mailboxes, websites, remote calendars or filesystems), depending on an interpretation, various kinds of data may be extracted from such an address.</rdfs:comment>
	<rdfs:subClassOf rdf:resource="&nie;DataObject"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;Software"
	 rdfs:label="Software">
	<rdfs:comment>A piece of software. Examples may include applications and the operating system. This interpretation most commonly applies to SoftwareItems.</rdfs:comment>
	<rdfs:subClassOf rdf:resource="&nie;InformationElement"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;SoftwareItem"
	 rdfs:label="SoftwareItem">
	<rdfs:comment>A DataObject representing a piece of software. Examples of interpretations of a SoftwareItem include an Application and an OperatingSystem.</rdfs:comment>
	<rdfs:subClassOf rdf:resource="&nie;DataObject"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;SoftwareService"
	 rdfs:label="SoftwareService">
	<rdfs:comment>A service published by a piece of software, either by an operating system or an application. Examples of such services may include calendar, addresbook and mailbox managed by a PIM application. This category is introduced to distinguish between data available directly from the applications (Via some Interprocess Communication Mechanisms) and data available from files on a disk. In either case both DataObjects would receive a similar interpretation (e.g. a Mailbox) and wouldn't differ on the content level.</rdfs:comment>
	<rdfs:subClassOf rdf:resource="&nie;DataObject"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;SourceCode"
	 rdfs:comment="Code in a compilable or interpreted programming language."
	 rdfs:label="SourceCode">
	<rdfs:subClassOf rdf:resource="&nfo;PlainTextDocument"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;Spreadsheet"
	 rdfs:label="Spreadsheet">
	<rdfs:comment>A spreadsheet, created by a spreadsheet application. Examples might include Gnumeric, OpenOffice Calc or MS Excel.</rdfs:comment>
	<rdfs:subClassOf rdf:resource="&nfo;Document"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;TextDocument"
	 rdfs:comment="A text document"
	 rdfs:label="TextDocument">
	<rdfs:subClassOf rdf:resource="&nfo;Document"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;Trash"
	 rdfs:label="Trash">
	<rdfs:comment>Represents a container for deleted files, a feature common in modern operating systems.</rdfs:comment>
	<rdfs:subClassOf rdf:resource="&nfo;DataContainer"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;VectorImage"
	 rdfs:label="VectorImage">
	<rdfs:subClassOf rdf:resource="&nfo;Image"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;Video"
	 rdfs:comment="A video file."
	 rdfs:label="Video">
	<rdfs:subClassOf rdf:resource="&nfo;Visual"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;Visual"
	 rdfs:comment="File containing visual content."
	 rdfs:label="Visual">
	<rdfs:subClassOf rdf:resource="&nfo;Media"/>
</rdfs:Class>
<rdfs:Class rdf:about="&nfo;Website"
	 rdfs:label="Website">
	<rdfs:comment>A website, usually a container for remote resources, that may be interpreted as HTMLDocuments, images or other types of content.</rdfs:comment>
	<rdfs:subClassOf rdf:resource="&nie;InformationElement"/>
</rdfs:Class>
<rdf:Property rdf:about="&nfo;aspectRatio"
	 
	 rdfs:comment="Visual content aspect ratio. (Width divided by Height)"
	 rdfs:label="aspectRatio">
	<rdfs:domain rdf:resource="&nfo;Visual"/>
	<rdfs:range rdf:resource="&xsd;float"/>
</rdf:Property>
<rdf:Property rdf:about="&nfo;averageBitrate"
	 
	 rdfs:label="averageBitrate">
	<rdfs:comment>The average overall bitrate of a media container. (i.e. the size of the piece of media in bits, divided by it's duration expressed in seconds).</rdfs:comment>
	<rdfs:domain rdf:resource="&nfo;Media"/>
	<rdfs:subPropertyOf rdf:resource="&nfo;rate"/>
	<rdfs:range rdf:resource="&xsd;float"/>
</rdf:Property>
<rdf:Property rdf:about="&nfo;belongsToContainer"
	 
	 rdfs:label="belongsToContainer">
	<rdfs:comment>Models the containment relations between Files and Folders (or CompressedFiles).</rdfs:comment>
	<rdfs:domain rdf:resource="&nie;DataObject"/>
	<rdfs:subPropertyOf rdf:resource="&nie;isPartOf"/>
	<rdfs:range rdf:resource="&nfo;DataContainer"/>
</rdf:Property>
<rdf:Property rdf:about="&nfo;bitDepth"
	 
	 rdfs:label="bitDepth">
	<rdfs:comment>A common superproperty for all properties signifying the amount of bits for an atomic unit of data. Examples of subproperties may include bitsPerSample and bitsPerPixel</rdfs:comment>
	<rdfs:domain rdf:resource="&nfo;Media"/>
	<rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdf:Property rdf:about="&nfo;bitrateType"
	 
	 rdfs:comment="The type of the bitrate. Examples may include CBR and VBR."
	 rdfs:label="bitrateType">
	<rdfs:domain rdf:resource="&nfo;Media"/>
	<rdfs:range rdf:resource="&xsd;string"/>
</rdf:Property>
<rdf:Property rdf:about="&nfo;bitsPerSample"
	 
	 rdfs:comment="Amount of bits in each audio sample."
	 rdfs:label="bitsPerSample">
	<rdfs:domain rdf:resource="&nfo;Audio"/>
	<rdfs:subPropertyOf rdf:resource="&nfo;bitDepth"/>
	<rdfs:range rdf:resource="&xsd;integer"/>
</rdf:Property>
<rdf:Property rdf:about="&nfo;channels"
	 
	 rdfs:label="channels">
	<rdfs:comment>Number of channels. This property is to be used directly if no detailed information is necessary. Otherwise use more detailed subproperties.</rdfs:comment>
	<rdfs:domain rdf:resource="&nfo;Audio"/>
	<rdfs:range rdf:resource="&xsd;integer"/>
</rdf:Property>
<rdf:Property rdf:about="&nfo;characterCount"
	 
	 rdfs:comment="The amount of characters in the document."
	 rdfs:label="characterCount">
	<rdfs:domain rdf:resource="&nfo;TextDocument"/>
	<rdfs:range rdf:resource="&xsd;integer"/>
</rdf:Property>
<rdf:Property rdf:about="&nfo;codec"
	 
	 rdfs:comment="The name of the codec necessary to decode a piece of media."
	 rdfs:label="codec">
	<rdfs:domain rdf:resource="&nfo;Media"/>
	<rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdf:Property rdf:about="&nfo;colorDepth"
	 
	 rdfs:comment="Amount of bits used to express the color of each pixel."
	 rdfs:label="colorDepth">
	<rdfs:domain rdf:resource="&nfo;Visual"/>
	<rdfs:subPropertyOf rdf:resource="&nfo;bitDepth"/>
	<rdfs:range rdf:resource="&xsd;integer"/>
</rdf:Property>
<rdf:Property rdf:about="&nfo;commentCharacterCount"
	 
	 rdfs:label="commentCharacterCount">
	<rdfs:comment>The amount of character in comments i.e. characters ignored by the compiler/interpreter.</rdfs:comment>
	<rdfs:domain rdf:resource="&nfo;SourceCode"/>
	<rdfs:range rdf:resource="&xsd;integer"/>
</rdf:Property>
<rdf:Property rdf:about="&nfo;compressionType"
	 
	 rdfs:label="compressionType">
	<rdfs:comment>The type of the compression. Values include, 'lossy' and 'lossless'.</rdfs:comment>
	<rdfs:range rdf:resource="&nfo;CompressionType"/>
	<rdfs:domain rdf:resource="&nfo;Media"/>
</rdf:Property>
<rdf:Property rdf:about="&nfo;conflicts"
	 
	 rdfs:label="conflicts">
	<rdfs:comment>States that a piece of software is in conflict with another piece of software.</rdfs:comment>
	<rdfs:domain rdf:resource="&nfo;Software"/>
	<rdfs:range rdf:resource="&nfo;Software"/>
</rdf:Property>
<rdf:Property rdf:about="&nfo;count"
	 
	 rdfs:label="count">
	<rdfs:comment>A common superproperty for all properties signifying the amount of atomic media data units. Examples of subproperties may include sampleCount and frameCount.</rdfs:comment>
	<rdfs:domain rdf:resource="&nfo;Media"/>
	<rdfs:range rdf:resource="&xsd;integer"/>
</rdf:Property>
<rdf:Property rdf:about="&nfo;definesClass"
	 
	 rdfs:comment="Name of a class defined in the source code file."
	 rdfs:label="definesClass">
	<rdfs:domain rdf:resource="&nfo;SourceCode"/>
	<rdfs:range rdf:resource="&xsd;string"/>
</rdf:Property>
<rdf:Property rdf:about="&nfo;definesFunction"
	 
	 rdfs:label="definesFunction">
	<rdfs:comment>A name of a function/method defined in the given source code file.</rdfs:comment>
	<rdfs:domain rdf:resource="&nfo;SourceCode"/>
	<rdfs:range rdf:resource="&xsd;string"/>
</rdf:Property>
<rdf:Property rdf:about="&nfo;definesGlobalVariable"
	 
	 rdfs:label="definesGlobalVariable">
	<rdfs:comment>Name of a global variable defined within the source code file.</rdfs:comment>
	<rdfs:domain rdf:resource="&nfo;SourceCode"/>
	<rdfs:range rdf:resource="&xsd;string"/>
</rdf:Property>
<rdf:Property rdf:about="&nfo;deletionDate"
	 
	 rdfs:comment="The date and time of the deletion."
	 rdfs:label="deletionDate">
	<rdfs:domain rdf:resource="&nfo;DeletedResource"/>
	<rdfs:range rdf:resource="&xsd;dateTime"/>
</rdf:Property>
<rdf:Property rdf:about="&nfo;duration"
	 
	 rdfs:comment="Duration of a media piece."
	 rdfs:label="duration">
	<rdfs:domain rdf:resource="&nfo;Media"/>
	<rdfs:range rdf:resource="&xsd;duration"/>
</rdf:Property>
<rdf:Property rdf:about="&nfo;encoding"
	 
	 rdfs:label="encoding">
	<rdfs:comment>The encoding used for the Embedded File. Examples might include BASE64 or UUEncode</rdfs:comment>
	<rdfs:domain rdf:resource="&nfo;EmbeddedFileDataObject"/>
	<rdfs:range rdf:resource="&xsd;string"/>
</rdf:Property>
<rdf:Property rdf:about="&nfo;fileCreated"
	 
	 rdfs:comment="File creation date"
	 rdfs:label="fileCreated">
	<rdfs:subPropertyOf rdf:resource="&nie;created"/>
	<rdfs:domain rdf:resource="&nfo;FileDataObject"/>
	<rdfs:range rdf:resource="&xsd;dateTime"/>
</rdf:Property>
<rdf:Property rdf:about="&nfo;fileLastAccessed"
	 
	 rdfs:comment="Time when the file was last accessed."
	 rdfs:label="fileLastAccessed">
	<rdfs:domain rdf:resource="&nfo;FileDataObject"/>
	<rdfs:subPropertyOf rdf:resource="&dc;date"/>
	<rdfs:range rdf:resource="&xsd;dateTime"/>
</rdf:Property>
<rdf:Property rdf:about="&nfo;fileLastModified"
	 rdfs:comment="last modification date"
	 rdfs:label="fileLastModified">
	<rdfs:domain rdf:resource="&nfo;FileDataObject"/>
	<rdfs:subPropertyOf rdf:resource="&dc;date"/>
	<rdfs:range rdf:resource="&xsd;dateTime"/>
</rdf:Property>
<rdf:Property rdf:about="&nfo;fileName"
	 rdfs:label="fileName">
	<rdfs:comment>Name of the file, together with the extension</rdfs:comment>
	<rdfs:domain rdf:resource="&nfo;FileDataObject"/>
	<rdfs:range rdf:resource="&xsd;string"/>
</rdf:Property>
<rdf:Property rdf:about="&nfo;fileOwner"
	 
	 rdfs:label="fileOwner">
	<rdfs:comment>The owner of the file as defined by the file system access rights feature.</rdfs:comment>
	<rdfs:range rdf:resource="&nco;Contact"/>
	<rdfs:domain rdf:resource="&nfo;FileDataObject"/>
</rdf:Property>
<rdf:Property rdf:about="&nfo;fileSize"
	 
	 rdfs:label="fileSize">
	<rdfs:comment>The size of the file in bytes. For compressed files it means the size of the packed file, not of the contents. For folders it means the aggregated size of all contained files and folders </rdfs:comment>
	<rdfs:subPropertyOf rdf:resource="&nie;byteSize"/>
	<rdfs:domain rdf:resource="&nfo;FileDataObject"/>
	<rdfs:range rdf:resource="&xsd;integer"/>
</rdf:Property>
<rdf:Property rdf:about="&nfo;fileUrl"
	 rdfs:label="fileUrl">
	<rdfs:comment>URL of the file. It points at the location of the file. In cases where creating a simple file:// or http:// URL for a file is difficult (e.g. for files inside compressed archives) the applications are encouraged to use conventions defined by Apache Commons VFS Project at http://jakarta.apache.org/  commons/ vfs/ filesystems.html.</rdfs:comment>
	<rdfs:domain rdf:resource="&nfo;FileDataObject"/>
	<rdfs:range rdf:resource="&rdfs;Resource"/>
</rdf:Property>
<rdf:Property rdf:about="&nfo;fontFamily"
	 
	 rdfs:comment="The name of the font family."
	 rdfs:label="fontFamily">
	<rdfs:domain rdf:resource="&nfo;Font"/>
	<rdfs:range rdf:resource="&xsd;string"/>
</rdf:Property>
<rdf:Property rdf:about="&nfo;foundry"
	 
	 rdfs:comment="The foundry, the organization that created the font."
	 rdfs:label="foundry">
	<rdfs:range rdf:resource="&nco;Contact"/>
	<rdfs:subPropertyOf rdf:resource="&nco;creator"/>
	<rdfs:domain rdf:resource="&nfo;Font"/>
</rdf:Property>
<rdf:Property rdf:about="&nfo;frameCount"
	 
	 rdfs:comment="The amount of frames in a video sequence."
	 rdfs:label="frameCount">
	<rdfs:domain rdf:resource="&nfo;Video"/>
	<rdfs:subPropertyOf rdf:resource="&nfo;count"/>
	<rdfs:range rdf:resource="&xsd;integer"/>
</rdf:Property>
<rdf:Property rdf:about="&nfo;frameRate"
	 
	 rdfs:comment="Amount of video frames per second."
	 rdfs:label="frameRate">
	<rdfs:domain rdf:resource="&nfo;Video"/>
	<rdfs:subPropertyOf rdf:resource="&nfo;rate"/>
	<rdfs:range rdf:resource="&xsd;float"/>
</rdf:Property>
