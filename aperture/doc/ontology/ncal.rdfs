	 rdfs:label="cancelledEventStatus"/>
<ncal:JournalStatus rdf:about="&ncal;cancelledJournalStatus"
	 rdfs:label="cancelledJournalStatus"/>
<ncal:TodoStatus rdf:about="&ncal;cancelledTodoStatus"
	 rdfs:label="cancelledTodoStatus"/>	 
<ncal:JournalStatus rdf:about="&ncal;finalStatus"
	 rdfs:label="finalStatus"/>
<ncal:AttendeeRole rdf:about="&ncal;chairRole"
	 rdfs:label="chairRole"/>
<ncal:ParticipationStatus rdf:about="&ncal;completedParticipationStatus"
	 rdfs:label="completedParticipationStatus"/>
<ncal:TodoStatus rdf:about="&ncal;completedStatus"
	 rdfs:label="completedStatus"/>
<ncal:AccessClassification rdf:about="&ncal;confidentialClassification"
	 rdfs:label="confidentialClassification"/>
<ncal:EventStatus rdf:about="&ncal;confirmedStatus"
	 rdfs:label="confirmedStatus"/>
<ncal:RecurrenceFrequency rdf:about="&ncal;daily"
	 rdfs:label="daily"/>
<ncal:ParticipationStatus rdf:about="&ncal;declinedParticipationStatus"
	 rdfs:label="declinedParticipationStatus"/>
<ncal:ParticipationStatus rdf:about="&ncal;delegatedParticipationStatus"
	 rdfs:label="delegatedParticipationStatus"/>
<ncal:AlarmAction rdf:about="&ncal;displayAction"
	 rdfs:label="displayAction"/>
<ncal:JournalStatus rdf:about="&ncal;draftStatus"
	 rdfs:label="draftStatus"/>
<ncal:AlarmAction rdf:about="&ncal;emailAction"
	 rdfs:label="emailAction"/>
<ncal:TriggerRelation rdf:about="&ncal;endTriggerRelation"
	 rdfs:label="endTriggerRelation"/>
<ncal:FreebusyType rdf:about="&ncal;freeFreebusyType"
	 rdfs:label="freeFreebusyType"/>
<ncal:Weekday rdf:about="&ncal;friday"
	 rdfs:label="friday"/>
<ncal:CalendarScale rdf:about="&ncal;gregorianCalendarScale"
	 rdfs:label="gregorianCalendarScale"/>
<ncal:CalendarUserType rdf:about="&ncal;groupUserType"
	 rdfs:label="groupUserType"/>
<ncal:RecurrenceFrequency rdf:about="&ncal;hourly"
	 rdfs:label="hourly"/>
<ncal:ParticipationStatus rdf:about="&ncal;inProcessParticipationStatus"
	 rdfs:label="inProcessParticipationStatus"/>
<ncal:TodoStatus rdf:about="&ncal;inProcessStatus"
	 rdfs:label="inProcessStatus"/>
