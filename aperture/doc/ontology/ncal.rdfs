	 rdfs:label="acceptedParticipationStatus"/>
<ncal:AlarmAction rdf:about="&ncal;audioAction"
	 rdfs:label="audioAction"/>
<ncal:AttachmentEncoding rdf:about="&ncal;base64Encoding"
	 rdfs:label="base64Encoding"/>
