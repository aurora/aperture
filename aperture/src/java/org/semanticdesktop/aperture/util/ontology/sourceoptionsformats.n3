@prefix fresnel:  <http://www.w3.org/2004/09/fresnel#> .
@prefix foaf:     <http://xmlns.com/foaf/0.1/> .
@prefix rdf:      <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd:      <http://www.w3.org/2001/XMLSchema#> .
@prefix source:   <http://aperture.semanticdesktop.org/ontology/source#> .
@prefix sourceformat: <http://aperture.semanticdesktop.org/ontology/sourceformat#> .
@prefix : <http://aperture.semanticdesktop.org/ontology/datasourcens#> .

:basepathFormat rdf:type fresnel:Format ;
                fresnel:propertyFormatDomain source:hostname ;
                sourceformat:valueWidget sourceformat:TextFieldWidget .

:includeEmbeddedResourcesFormat rdf:type fresnel:Format ;
                                fresnel:propertyFormatDomain source:includeEmbeddedResources ;
                                sourceformat:valueWidget [ a sourceformat:CheckBoxWidget ] .

:includeHiddenResourcesFormat rdf:type fresnel:Format ;
                              fresnel:propertyFormatDomain source:includeHiddenResources ;
                              sourceformat:valueWidget [ a sourceformat:CheckBoxWidget ] .                    
                                
:maximumDepthFormat rdf:type fresnel:Format ;
                    fresnel:propertyFormatDomain source:maximumDepth ;
                    sourceformat:valueWidget [ a sourceformat:IntegerFieldWidget ] .
                    
:maximumByteSizeFormat rdf:type fresnel:Format ;
                       fresnel:propertyFormatDomain source:maximumByteSize ;
                       sourceformat:valueWidget [ a sourceformat:IntegerFieldWidget ] .

:rootURLFormat rdf:type fresnel:Format ;
               fresnel:propertyFormatDomain source:rootURL ;
               sourceformat:valueWidget [ a sourceformat:TextFieldWidget ] .
                       