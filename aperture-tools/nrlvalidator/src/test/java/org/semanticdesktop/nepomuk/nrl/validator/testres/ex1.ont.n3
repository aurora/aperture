@prefix nrl:     <http://www.semanticdesktop.org/ontologies/2006/11/24/nrl#> .
@prefix nao:     <http://www.semanticdesktop.org/ontologies/2007/03/nao#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix ex:  <http://www.abc.com/ex#> .

ex:Vehicle
          a       rdfs:Class .

    ex:drives
          a       rdf:Property ;
          rdfs:domain ex:Person ;
          rdfs:range ex:Vehicle .

    ex:Person
          a       rdfs:Class ;
          rdfs:isDefinedBy "http://www.world.com#somethingstupid" .
   

