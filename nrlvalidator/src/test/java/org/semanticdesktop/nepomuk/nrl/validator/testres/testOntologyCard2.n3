@prefix : <http://example.org/inst1#> .
@prefix ont1: <http://example.org/ontology1#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix nrl: <http://www.semanticdesktop.org/ontologies/2007/08/15/nrl#> . 
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ont1:Class1 a rdfs:Class .
ont1:Class2 a rdfs:Class .

# Error, datatype undefined
ont1:Property1 a rdf:Property ;
     rdfs:domain ont1:Class1 ;
     rdfs:range  ont1:Class2 ;
		 nrl:cardinality "1" .

# no error, minimum cardinality can be zero
ont1:Property2 a rdf:Property ;
     rdfs:domain ont1:Class1 ;
     rdfs:range  ont1:Class2 ;
		 nrl:minCardinality "0"^^xsd:nonNegativeInteger .

# Error, maximum cardinality is not positive
ont1:Property3 a rdf:Property ;
     rdfs:domain ont1:Class1 ;
     rdfs:range  ont1:Class2 ;
		 nrl:maxCardinality "0"^^xsd:nonNegativeInteger .
		 
# Error, minimum cardinality higher than maximum cardinality
ont1:Property4 a rdf:Property ;
     rdfs:domain ont1:Class1 ;
     rdfs:range  ont1:Class2 ;
     nrl:minCardinality "3"^^xsd:nonNegativeInteger ;
		 nrl:maxCardinality "2"^^xsd:positiveInteger .

